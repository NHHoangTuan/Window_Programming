<Window x:Class="EnglishVocabulary.QuizWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EnglishVocabulary"
        mc:Ignorable="d"
        Title="QuizWindow" Height="450" Width="800">
    <Canvas>
        <Frame x:Name="QuizFrame" NavigationUIVisibility="Hidden" Height="379" Canvas.Top="45" Width="800" HorizontalAlignment="Center" VerticalAlignment="Top"/>

        <Button x:Name="btnGuide" Content="HDSD" Width="50" Height="30" Canvas.Top="10" Canvas.Left="10"
                Click="btnGuide_Click"/>

        <Button x:Name="btnStart" Content="Start" Width="50" Height="30" Canvas.Top="10" Canvas.Left="65"
        Click="btnStart_Click"/>

        <Button x:Name="btnClose" Content="Close" Width="50" Height="30" Canvas.Top="10" Canvas.Left="120"
        Click="btnClose_Click" Visibility="Collapsed"/>


    </Canvas>
</Window>
